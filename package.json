{
  "name": "alter-analytics-backend",
  "version": "1.0.0",
  "description": "Cleaned analytics backend (Express + Postgres + Redis)",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec node src/index.js",
    "start": "node src/index.js",
    "lint": "eslint src --ext .js --fix",
    "format": "prettier --write 'src/**/*.{js,json,md}'",
    "test": "echo \"No tests configured\" && exit 0",
    "migrate": "psql -U postgres -d analyticsdb -f migrations/001_init.sql"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
